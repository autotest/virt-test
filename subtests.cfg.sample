                nx_on = yes
            - noexec_off:
                cpu_model = qemu64
                extra_flags = -nx
                nx_on = no

    - yonit_bitmap_benchmark:
        only Windows
        type = yonit_bitmap
        test_name = "yonit_bitmap_benchmark"
        reboot = no
        test_timeout = 86400
        download = yes
        download_cmd = "git clone"
        rsc_server = "git://qe-git.englab.nay.redhat.com/s2/yonit_bitmap_benchmark"
        dst_rsc_dir = "C:\"
        dst_rsc_path = "C:\yonit_bitmap_benchmark\prod"
